<!DOCTYPE html>
<html>
<head>
  <title>CapstoneProject</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'shared/notice_and_errors' %>

<% if logged_in? %>
  Signed in as <%= current_user.email %> <%= button_to "sign out", session_url, method: "delete" %>
<% else %>
  <%= link_to "Please sign in.", new_session_url %>
<% end %>

<%= yield %>

</body>
</html>
